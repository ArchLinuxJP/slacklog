  },
  {
    "user": "U53RAQBCP",
    "text": ":goodpoem: ",
    "client_msg_id": "633339A3-6A47-4B4A-B078-E75746B4531C",
    "name": "kokopis",
    "img": "https://avatars.slack-edge.com/2021-02-17/1758258964165_98bf0ec770bc4e813fb0_24.png",
